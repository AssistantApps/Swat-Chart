{
  "name": "@assistantapps/swat-chart",
  "version": "0.0.3",
  "description": "Swat chart made for use in AssistantApps and other projects",
  "license": "MIT",
  "author": "Kurt Lourens",
  "scripts": {
    "setup": "git config core.hooksPath .githooks/",
    "report": "bun ./src/data/report.ts",
    "prebuild": "bun ./src/data/preBuild.ts",
    "build": "tsup",
    "quick-update": "npx npm-check-updates -u && npm i --force && bun install && npm run report",
    "test": "vitest --ui --project unit"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "solid-js": "^1.9.7",
    "tsup": "^8.5.0",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-solid": "^2.11.7",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AssistantApps/Swat-Chart.git"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {},
  "keywords": [
    "assistantapps",
    "swat",
    "swat-chart",
    "chart"
  ]
}